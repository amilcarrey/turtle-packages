{
   "name": "@turtledev/earn-wizzard",
   "version": "0.1.0",
   "description": "Configurable widget wizard for Turtle Earn platform",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/turtledev/packages.git",
      "directory": "packages/earn-wizzard"
   },
   "sideEffects": false,
   "type": "module",
   "exports": {
      ".": {
         "types": "./types/index.d.ts",
         "import": "./dist/es/index.js",
         "require": "./dist/cjs/index.js"
      }
   },
   "main": "./dist/cjs/index.js",
   "module": "./dist/es/index.js",
   "types": "./types/index.d.ts",
   "scripts": {
      "build": "rm -rf dist && rm -rf types && bun run build:code && bun run build:types",
      "build:code": "vite build",
      "build:types": "tsc -p .",
      "type-check": "tsc --noEmit",
      "dev": "vite build --watch"
   },
   "dependencies": {
      "@storybook/addon-docs": "9.0.13",
      "@storybook/blocks": "^8.6.14",
      "@storybook/react": "9.0.13",
      "@turtledev/ui": "workspace:*",
      "class-variance-authority": "^0.7.1",
      "jotai": "^2.10.3",
      "lucide-react": "^0.460.0"
   },
   "peerDependencies": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0"
   },
   "devDependencies": {
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0",
      "typescript": "^5.0.0",
      "vite": "^5.0.0"
   },
   "publishConfig": {
      "access": "public"
   }
}