{
   "name": "@turtledev/earn-wizzard",
   "version": "0.1.0",
   "description": "Configurable widget wizard for Turtle Earn platform",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/turtledev/packages.git",
      "directory": "packages/earn-wizzard"
   },
   "sideEffects": false,
   "type": "module",
   "exports": {
      ".": {
         "types": "./types/index.d.ts",
         "import": "./dist/esm/index.js",
         "require": "./dist/cjs/index.js"
      }
   },
   "main": "./dist/cjs/index.js",
   "module": "./dist/esm/index.js",
   "types": "./types/index.d.ts",
   "scripts": {
      "build": "rm -rf dist && rm -rf types && bun run build:code && bun run build:types",
      "build:code": "rollup -c --bundleConfigAsCjs",
      "build:types": "tsc -p .",
      "type-check": "tsc --noEmit"
   },
   "dependencies": {
      "lucide-react": "^0.460.0",
      "class-variance-authority": "^0.7.1",
      "jotai": "^2.10.3"
   },
   "peerDependencies": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0"
   },
   "devDependencies": {
      "@babel/core": "^7.27.4",
      "@babel/preset-react": "^7.27.1",
      "@babel/preset-typescript": "^7.27.1",
      "@rollup/plugin-alias": "^5.1.1",
      "@rollup/plugin-babel": "^6.0.4",
      "@rollup/plugin-commonjs": "^28.0.6",
      "@rollup/plugin-node-resolve": "^16.0.1",
      "@rollup/plugin-typescript": "^12.1.3",
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.0",
      "rollup": "^4.44.0",
      "typescript": "^5.0.0"
   },
   "publishConfig": {
      "access": "public"
   }
}